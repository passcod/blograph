<% posts.forEach(({ post }) => { %>
<article class="h-entry">
  <h1><a href="<%= post.slug %>" class="p-name u-url"><%= post.title %></a></h1>
  <p class="p-summary">
    <date datetime="<%= post.date.rfc3339 %>" class="dt-published">
      <%= moment(post.date).format('MMM D ‘YY') + ' ' %>
    </date>
    — <%- post.render.substr(0, 140).trim() %>…</p>
</article>
<% }); %>
